<!doctype html>
<html lang="en">
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Backbone.js • TodoMVC</title>
  <link href="/css/bootstrap.css" rel="stylesheet">
  <link href="/css/custom.css" rel="stylesheet">
  <link href="/css/bootstrap-responsive.css" rel="stylesheet">
  
  <!-- HTML5 shim, for IE6-8 support of HTML5 elements -->
  <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
  <![endif]-->
</head>
<body>
  
  <script type="text/template" id="menu-item-template-old">
    <div id="read-only-link-{{_id}}">
      <a href="#pageByUrl/{{pageUrl}}" > {{label}} </a>
  	  <a href="#edit/link/{{_id}}" class="edit-link">[Edit]</a>
    </div>
    <div id="edit-link-{{_id}}" style="display: none">
      <input type="text" id="label" name="label" value="{{label}}"/>
      <button class="save-link-button" name="save-link">Save</button>
      <button class="cancel-link-button" name="cancel-link">Cancel</button>
    </div>
  </script>


  <script type="text/template" id="menu-item-template">
    <a href="#pageByUrl/{{pageUrl}}" > {{label}} </a>
  </script>

  <script type="text/template" id="page-template">
    <div id="read-only-page-{{_id}}" class="jumbotron">
      <h1>{{h1}}</h1>
      <p class="lead">{{p}}</p>
      <a href="#edit/page/{{_id}}" class="edit-page"> [Edit] </a>
      <a href="#remove/page/{{_id}}" class="remove-page"> [Remove] </a>
    </div>
    <div id="edit-page-{{_id}}"  style="display: none">
      <div class="control-group">
        <label for="h1" class="control-label">H1</label>
        <div class="controls">
          <input type="text" id="h1" name="h1" value="{{h1}}" class="span7"/>
        </div>
      </div>

      <div class="control-group">
        <label for="p" class="control-label">p</label>
        <div class="controls">
          <input type="text" id="p" name="p" value="{{p}}"/>
        </div>
      </div>

      <div class="control-group">
        <label for="p" class="control-label">p</label>
        <div class="controls">
          <input type="checkbox" name="large" value="{{large}}" />
        </div>
      </div>



      <input type="hidden" id="pageUrl" name="pageUrl" value="{{pageUrl}}"/>
      <button class="save-page-button btn btn-primary" name="save-page">Save</button>
      <button class="cancel-page-button btn" name="cancel-page">Cancel</button>
    </div>
    <hr>
  </script>

  <div class="container-narrow">
    
    <!-- Menu-->
    <div class="navbar navbar-inverse navbar-fixed-top">
      <div class="navbar-inner" >
        <div class="container">
          <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </a>
          <a class="brand" href="#">MJS |</a>
          <div class="nav-collapse collapse" id="menu-container"></div>
        </div>
      </div>
    </div>
   

    
    <div id="add-page-container"></div>  
    <div id="page-container"></div>

  </div>

  <!-- Admin panel-->
  <div class="navbar navbar-fixed-bottom" id="admin-page">
    <a href="#create/page">Create page</a>
    <a href="#create/link">Create link</a>
  </div>

  <!-- Button to trigger modal -->
<a href="#myModal" role="button" class="btn" data-toggle="modal">Launch demo modal</a>
 
<!-- Modal -->
<div id="edit">
<div id="myModal" class="modal hide fade" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
  <div class="modal-header">
    <button type="button" class="close" data-dismiss="modal" aria-hidden="true">×</button>
    <h3 id="myModalLabel">Modal header</h3>
  </div>
  <div class="modal-body">
    <p>One fine body…</p>
  </div>
  <div class="modal-footer">
    <button class="btn" data-dismiss="modal" aria-hidden="true">Close</button>
    <button class="btn btn-primary">Save changes</button>
  </div>
</div>
</div>


  
  <script src="js/lib/jquery.js"></script>
  <script src="js/lib/underscore.js"></script>
  <script src="js/lib/backbone.js"></script>
  <script src="js/lib/bootstrap.js"></script>
  
  <script src="js/models/Link.js"></script>
  <script src="js/models/Page.js"></script>
  
  <script src="js/collections/Link.js"></script>
  <script src="js/collections/Page.js"></script>

  <script src="js/views/Link.js"></script>
  <script src="js/views/LinkList.js"></script>
  
  <script src="js/views/Page.js"></script>
  <script src="js/views/PageList.js"></script>
  
  <script src="js/routers/router.js"></script>
  
  <script src="js/app.js"></script>  
</body>
</html>